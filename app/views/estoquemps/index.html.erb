<p id="notice"><%= notice %></p>

<h1>Listing Estoquemps</h1>

<table>
  <thead>
    <tr>
      <th>Id cervejaria</th>
      <th>Item</th>
      <th>Quantidade atual</th>
      <th>Destino</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @estoquemps.each do |estoquemp| %>
      <tr>
        <td><%= estoquemp.id_cervejaria %></td>
        <td><%= estoquemp.item %></td>
        <td><%= estoquemp.quantidade_atual %></td>
        <td><%= estoquemp.destino %></td>
        <td><%= link_to 'Show', estoquemp %></td>
        <td><%= link_to 'Edit', edit_estoquemp_path(estoquemp) %></td>
        <td><%= link_to 'Destroy', estoquemp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Estoquemp', new_estoquemp_path %>
